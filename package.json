{
    "name": "@clearblade/asset-monitor",
    "version": "3.7.7",
    "description": "Asset Monitor Biolerplate",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watchAll",
        "prepublish": "tsc",
        "prebuild": "del lib/",
        "build": "tsc",
        "bundle": "webpack",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "pre-commit": [
        "lint",
        "test"
    ],
    "lint-staged": {
        "*.{ts}": "npm run lint"
    },
    "keywords": [],
    "author": "yashjain28",
    "license": "ISC",
    "types": "index.d.ts",
    "main": "src/index.js",
    "devDependencies": {
        "@babel/core": "^7.3.3",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/polyfill": "^7.6.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@clearblade/cb-dev-kit": "^1.1.3",
        "@clearblade/code-service-types": "^1.1.0",
        "@types/jest": "^24.0.6",
        "@types/node": "^11.9.4",
        "@types/react": "^16.8.10",
        "@types/uuid": "^3.4.6",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "ajv": "^6.10.2",
        "babel-jest": "^24.1.0",
        "babel-loader": "^8.0.5",
        "chalk": "^2.4.2",
        "chokidar": "^2.1.5",
        "cpy-cli": "^3.0.0",
        "del-cli": "^3.0.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^24.1.0",
        "jest-cli": "^24.1.0",
        "lint-staged": "^9.5.0",
        "npm-run-all": "^4.1.5",
        "pre-commit": "^1.2.2",
        "prettier": "^1.19.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "source-map-loader": "^0.2.4",
        "ts-jest": "^24.0.0",
        "typescript": "^3.3.3",
        "webpack": "^4.29.4",
        "webpack-cli": "^3.3.0"
    },
    "babel": {
        "presets": [
            "@babel/typescript",
            [
                "@babel/env",
                {
                    "modules": false
                }
            ]
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "diagnostics": false
            }
        },
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "transform": {
            "^.+\\.jsx?$": "babel-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "setupFiles": [
            "./src/setupTests.ts"
        ]
    },
    "directories": {
        "example": "examples",
        "lib": "lib"
    },
    "dependencies": {
        "@clearblade/messaging-utils": "^1.0.3",
        "@clearblade/one-way-sync": "^1.1.8",
        "@clearblade/promise-polyfill": "^1.0.0",
        "core-js": "^3.6.2",
        "json-rules-engine": "^5.0.0",
        "selectn": "^1.1.2",
        "uuid": "^3.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ClearBlade/asset-monitor.git"
    },
    "bugs": {
        "url": "https://github.com/ClearBlade/asset-monitor/issues"
    },
    "homepage": "https://github.com/ClearBlade/asset-monitor#readme"
